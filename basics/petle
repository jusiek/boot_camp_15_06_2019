min_l = None
max_l = None

while True:
    x = input(("wpisz liczbe lub k)")
            if x == 'k':
                break
            if min_l is None:
                min_l = int(x)
            if max_l is None:
                max_l = int(x)
    print(f"""
ZnalezioneMinimum to: {min_l}
ZnalezioneMAximum to: {max_l}
""")